<div class="details">
    <span class="filter">
        <a href="#">{{ job.city }}, {{ job.countryname }}</a> /
        <a href="#">{{ job.contracttype }}</a>
        
        <br/><span class="archived">Archived since {{ job.getCreatedAt|date('d/m/Y') }}</span>
    </span>
</div>

<a href="{{ path('job_show') }}" class="title">
    {{ job.title }}
    <span class="company">@&nbsp;{{ job.company }}</span>
</a>
